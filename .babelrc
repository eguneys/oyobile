{
  "plugins": [
    ["transform-object-rest-spread"],
    ["transform-react-jsx", {
      "pragma": "h"
    }],

    ["transform-react-require", {
      "identifier": "h",
      "moduleName": "mithril/hyperscript"
    }]
  ],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}